<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" 
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:type="TaskPaneApp">
  <Id>com.example.chordsaddin</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Your Name</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Chords Add-In"/>
  <Description DefaultValue="Easily add chords to song lyrics."/>
  <IconUrl DefaultValue="https://placeholder-url.com/icon.png"/>
  <SupportUrl DefaultValue="https://placeholder-url.com/support"/>
  
  <Hosts>
    <Host Name="Document"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="SharedRuntime" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <Resources>
    <bt:Images>
      <bt:Image Id="icon16x16" DefaultValue="https://placeholder-url.com/icon16.png"/>
      <bt:Image Id="icon32x32" DefaultValue="https://placeholder-url.com/icon32.png"/>
    </bt:Images>
    <bt:Urls>
      <bt:Url Id="TaskPaneUrl" DefaultValue="https://your-hosting-url.com/index.html"/>
    </bt:Urls>
  </Resources>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://SholomN.github.io/chords-addin/index.html
"/>
  </DefaultSettings>
</OfficeApp>
